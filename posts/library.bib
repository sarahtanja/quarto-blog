@article{mincarelli2021,
	title = {Consequences of combined exposure to thermal stress and the plasticiser {DEHP} in Mytilus spp. differ by sex},
	volume = {170},
	issn = {0025326X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0025326X21006585},
	doi = {10.1016/j.marpolbul.2021.112624},
	abstract = {Little is known about the combined effect of environmental factors and contaminants on commercially important marine species, and whether this effect differs by sex. In this study, blue mussels were exposed for seven days to both single and combined stressors (i.e., +3 ◦C elevated temperature and two environmentally relevant con­ centrations of the plastic softener {DEHP}, 0.5 and 50 μg/l) in a factorial design. Males were observed to be more sensitive to high temperature, demonstrated by the significant increase in out-of-season spawning gonads and higher gene expression of the antioxidant catalase and the estrogen receptor genes. On the other hand, while the gametogenesis cycle in females was more resilient than in males, {DEHP} exposure altered the estrogen-related receptor gene expression.},
	pages = {112624},
	journaltitle = {Marine Pollution Bulletin},
	author = {Mincarelli, Luana Fiorella and Rotchell, Jeanette M. and Chapman, Emma C. and Turner, Alexander P. and Wollenberg Valero, Katharina C.},
