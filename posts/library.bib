  author = {Zhan, Yu and Marchand, Christophe H. and Maes, Alexandre and Mauries, Adeline and Sun, Yi and Dhaliwal, James S. and Uniacke, James and Arragain, Simon and Jiang, Heng and Gold, Nicholas D. and Martin, Vincent J. J. and Lemaire, Stéphane D. and Zerges, William},
  date = {2018-02-26},
  journaltitle = {PLoS ONE},
  shortjournal = {PLoS One},
  volume = {13},
