@article{shoreFourPlasticAdditives2022,
  title = {Four Plastic Additives Reduce Larval Growth and Survival in the Sea Urchin {{Strongylocentrotus}} Purpuratus},
  author = {Shore, Emily A. and Huber, Kaitlin E. and Garrett, April D. and Pespeni, Melissa H.},
  date = {2022-02-02},
