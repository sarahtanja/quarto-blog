}

@article{berryMicroplasticContaminationHas2019,
  title = {Microplastic {{Contamination Has Limited Effects}} on {{Coral Fertilisation}} and {{Larvae}}},
  author = {Berry, Kathryn L. E. and Epstein, Hannah E. and Lewis, Phoebe J. and Hall, Nora M. and Negri, Andrew P.},
