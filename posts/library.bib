  shorttitle = {Genetic {{Comparisons}} of {{Freshly Isolated Versus Cultured Symbiotic Dinoflagellates}}},
  author = {Santos, Scott R. and Taylor, Derek J. and Coffroth, Mary Alice},
  date = {2001},
  journaltitle = {Journal of Phycology},
