@article{zhao2023,
  title = {Characteristics of Chromophoric Dissolved Organic Matter in the {{Northern Andaman Sea}}},
  author = {Zhao, Ruoshi and Li, Zhongqiao and Aye, Aung Aung and Zheng, Hao and Jin, Weifang and Zhang, Jingjing and Zhou, Feng and Li, Hongliang and Han, Nang Mya and Lwin, Myat and Chen, Jianfang},
