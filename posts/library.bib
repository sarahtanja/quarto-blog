@article{bohnesLifeCycleAssessments2019,
  title = {Life Cycle Assessments of Aquaculture Systems: A Critical Review of Reported Findings with Recommendations for Policy and System Development},
  shorttitle = {Life Cycle Assessments of Aquaculture Systems},
  author = {Bohnes, Florence Alexia and Hauschild, Michael Zwicky and Schlundt, Jørgen and Laurent, Alexis},
  date = {2019-11},
  journaltitle = {Reviews in Aquaculture},
  shortjournal = {Rev Aquacult},
  volume = {11},
