@article{eddy2018,
	title = {Historical baselines of coral cover on tropical reefs as estimated by expert opinion},
	volume = {6},
	issn = {2167-8359},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5786882/},
	doi = {10.7717/peerj.4308},
	abstract = {Coral reefs are important habitats that represent global marine biodiversity hotspots and provide important benefits to people in many tropical regions. However, coral reefs are becoming increasingly threatened by climate change, overfishing, habitat destruction, and pollution. Historical baselines of coral cover are important to understand how much coral cover has been lost, e.g., to avoid the ‘shifting baseline syndrome’. There are few quantitative observations of coral reef cover prior to the industrial revolution, and therefore baselines of coral reef cover are difficult to estimate. Here, we use expert and ocean-user opinion surveys to estimate baselines of global coral reef cover. The overall mean estimated baseline coral cover was 59\% (±19\% standard deviation), compared to an average of 58\% (±18\% standard deviation) estimated by professional scientists. We did not find evidence of the shifting baseline syndrome, whereby respondents who first observed coral reefs more recently report lower estimates of baseline coral cover. These estimates of historical coral reef baseline cover are important for scientists, policy makers, and managers to understand the extent to which coral reefs have become depleted and to set appropriate recovery targets.},
	pages = {e4308},
	journaltitle = {{PeerJ}},
	author = {Eddy, Tyler D. and Cheung, William W.L. and Bruno, John F.},
	urldate = {2022-03-16},
	date = {2018-01-24},
	pmid = {29379692},
	pmcid = {PMC5786882},
	file = {PubMed Central Full Text PDF:C\:\\Users\\Minerva\\Zotero\\storage\\46IM9PLS\\Eddy et al. - 2018 - Historical baselines of coral cover on tropical re.pdf:application/pdf},
}

@article{zaneveld_overfishing_2016,
	title = {Overfishing and nutrient pollution interact with temperature to disrupt coral reefs down to microbial scales},
