@article{zhu2022,
  title = {Microplastics Altered Soil Microbiome and Nitrogen Cycling: {{The}} Role of Phthalate Plasticizer},
  shorttitle = {Microplastics Altered Soil Microbiome and Nitrogen Cycling},
  author = {Zhu, Fengxiao and Yan, Yuanyuan and Doyle, Evelyn and Zhu, Changyin and Jin, Xin and Chen, Zhanghao and Wang, Chao and He, Huan and Zhou, Dongmei and Gu, Cheng},
