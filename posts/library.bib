@article{blackstoneWhyCoralsBleach2018,
  title = {Why {{Do Corals Bleach}}? {{Conflict}} and {{Conflict Mediation}} in a {{Host}}/{{Symbiont Community}}},
  shorttitle = {Why {{Do Corals Bleach}}?},
  author = {Blackstone, Neil W. and Golladay, Jeff M.},
  date = {2018},
  journaltitle = {BioEssays},
  volume = {40},
