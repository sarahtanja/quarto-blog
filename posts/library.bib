@article{brooksHistoricalControlData2019,
  title = {Historical Control Data for the Interpretation of Ecotoxicity Data: Are We Missing a Trick?},
  shorttitle = {Historical Control Data for the Interpretation of Ecotoxicity Data},
  author = {Brooks, Amy C. and Foudoulakis, Manousos and Schuster, Hanna S. and Wheeler, James R.},
  date = {2019-12-01},
  journaltitle = {Ecotoxicology},
