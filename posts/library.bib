  date = {2006-07-01},
  journaltitle = {Marine Biology},
  shortjournal = {Mar Biol},
  volume = {149},
  number = {4},
  pages = {713--719},
  issn = {1432-1793},
  doi = {10.1007/s00227-006-0272-x},
