@article{criddle_dysfunction_2013,
	title = {Dysfunction by design: Consequences of limitations on transferability of catch shares in the Alaska pollock fishery},
	volume = {40},
	issn = {0308597X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0308597X13000158},
	doi = {10.1016/j.marpol.2013.01.006},
	shorttitle = {Dysfunction by design},
	abstract = {The Eastern Bering Sea ﬁshery for pollock is one of the largest ﬁsheries in the world and has often been touted as an example of sustainable ﬁsheries management. Yet, sustainability requires more than protection of the biological productivity of the targeted ﬁsh species. It requires preservation of the ﬂows of net social and economic beneﬁts of the ﬁshery, and ﬂexible governance and management institutions that allow for adaptation to changes in ﬁsh abundance as well as changes in consumer demand and in the prices of key inputs. From an economic perspective, the Alaska pollock ﬁshery was unsustainable before passage of the American Fisheries Act ({AFA}) in 1998. The {AFA} permanently divided the pollock Total Allowable Catch ({TAC}) giving a 10\% allocation to the Community Development Quota ({CDQ}) program and partitioning the remainder among four ﬁshing sectors. It allowed {CDQ} to be leased to any sector but largely prohibited leasing or sale between other sectors. These limits on intersectoral transfers have reduced the ability of sectors to respond to changes in the abundance and distribution of pollock, implementation of management measures intended to beneﬁt other species, and changes in the prices of input factors and products. This paper explores these consequences.},
	pages = {91--99},
	journaltitle = {Marine Policy},
	author = {Criddle, Keith R. and Strong, James},
	urldate = {2022-12-12},
	date = {2013-07},
	langid = {english},
	file = {Criddle and Strong - 2013 - Dysfunction by design Consequences of limitations.pdf:C\:\\Users\\Minerva\\Zotero\\storage\\WUKACP94\\Criddle and Strong - 2013 - Dysfunction by design Consequences of limitations.pdf:application/pdf},
}

