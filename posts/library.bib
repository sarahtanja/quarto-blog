@article{dahlheim2022,
  title = {Interactions, Injuries, and Mortalities of Killer Whales ({{Orcinus}} Orca) Observed during Fishing Operations in {{Alaska}}},
  author = {Dahlheim, Marilyn E. and Cahalan, Jennifer and Breiwick, Jeffrey M.},
  date = {2022-03-24},
  journaltitle = {Fishery Bulletin},
