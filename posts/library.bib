@article{gunaalan2020,
  title = {The Hidden Threat of Plastic Leachates: {{A}} Critical Review on Their Impacts on Aquatic Organisms},
  shorttitle = {The Hidden Threat of Plastic Leachates},
  author = {Gunaalan, Kuddithamby and Fabbri, Elena and Capolupo, Marco},
  date = {2020-10-01},
  journaltitle = {Water Research},
  shortjournal = {Water Research},
  volume = {184},
  pages = {116170},
  issn = {0043-1354},
  doi = {10.1016/j.watres.2020.116170},
  url = {https://www.sciencedirect.com/science/article/pii/S0043135420307077},
  urldate = {2021-12-06},
  abstract = {Plastic products are made from the essential polymer mixed with a complex blend of substances including catalyst remnants, polymerization solvents, and a wide range of other additives deliberately added to enhance the desirable characteristics of the final product. Additives include bisphenols, phthalates, flame retardants, and further emerging and legacy contaminants. With a few exceptions, additives are not chemically bound to the polymer, and potentially migrate within the material reaching its surface, then possibly leach out to the environment. Leachates are mixtures of additives, some of which belong to the list of emerging contaminants, i.e. substances that show the potential to pose risks to the environment and human health, while are not yet regulated. The review discusses the state of the art and gaps concerning the hidden threat of plastic leachates. The focus is on reports addressing the biological impacts of plastic leachates as a whole mixture. Degradation of plastics, including the weathering-driven fragmentation, and the release of additives, are analysed together with the techniques currently employed for chemically screening leachates. Because marine plastic litter is a major concern, the review mainly focuses on the effects of plastic leachates on marine flora and fauna. Moreover, it also addresses impacts on freshwater organisms. Finally, research needs and perspectives are examined, to promote better focused investigations, that may support developing different plastic materials and new regulations.},
  langid = {english},
  keywords = {Aquatic organisms,Chemical screening,Ecotoxicity,Leachate preparation,Plastic additives},
  file = {C\:\\Users\\ssdon\\Zotero\\storage\\DHU983FR\\Gunaalan et al. - 2020 - The hidden threat of plastic leachates A critical.pdf;C\:\\Users\\ssdon\\Zotero\\storage\\ZLACPLX7\\S0043135420307077.html}
}

