  author = {Lichtenberg, Mads and Larkum, Anthony W. D. and Kühl, Michael},
  date = {2016-02-26},
  journaltitle = {Frontiers in Microbiology},
  shortjournal = {Front Microbiol},
  volume = {7},
  eprint = {26955372},
