  keywords = {Animals,Anthozoa,Atmosphere,Carbon Dioxide,Climate,Dinoflagellida,Ecosystem,Eukaryota,Fishes,Forecasting,Greenhouse Effect,Hydrogen-Ion Concentration,Oceans and Seas,Seawater,Temperature},
