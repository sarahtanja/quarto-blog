@article{burgos-acevesImpactPhthalatesBisphenols2021,
  title = {Impact of Phthalates and Bisphenols Plasticizers on Haemocyte Immune Function of Aquatic Invertebrates: {{A}} Review on Physiological, Biochemical, and Genomic Aspects},
  shorttitle = {Impact of Phthalates and Bisphenols Plasticizers on Haemocyte Immune Function of Aquatic Invertebrates},
