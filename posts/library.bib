  author = {Rothig, Till and Costa, Ruben M. and Simona, Fabia and Baumgarten, Sebastian and Torres, Ana F. and Radhakrishnan, Anand and Aranda, Manuel and Voolstra, Christian R.},
  date = {2016},
  journaltitle = {FRONTIERS IN MARINE SCIENCE},
  shortjournal = {Front. Mar. Sci.},
  volume = {3},
  pages = {234},
  publisher = {{Frontiers Media Sa}},
  location = {{Lausanne}},
  issn = {2296-7745},
