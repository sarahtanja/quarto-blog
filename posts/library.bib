}

@article{yin2021,
  title = {Acute Exposure of Di(2-Ethylhexyl) Phthalate ({{DEHP}}) Induces Immune Signal Regulation and Ferroptosis in Oryzias Melastigma},
  author = {Yin, Xiaohan and Zeb, Rabia and Wei, Hui and Cai, Ling},
  date = {2021-02-01},
  journaltitle = {Chemosphere},
  shortjournal = {Chemosphere},
