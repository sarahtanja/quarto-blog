	langid = {english},
	file = {Jambeck et al. - 2015 - Plastic waste inputs from land into the ocean.pdf:C\:\\Users\\Minerva\\Zotero\\storage\\S6N7B9HX\\Jambeck et al. - 2015 - Plastic waste inputs from land into the ocean.pdf:application/pdf},
}

