@article{biggsViralLysisModifies2021,
  title = {Viral Lysis Modifies Seasonal Phytoplankton Dynamics and Carbon Flow in the {{Southern Ocean}}},
  author = {Biggs, Tristan E. G. and Huisman, Jef and Brussaard, Corina P. D.},
  date = {2021-12},
  journaltitle = {The ISME Journal},
  shortjournal = {ISME J},
  volume = {15},
