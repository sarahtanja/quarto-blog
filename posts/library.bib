	pmid = {19528054},
	pmcid = {PMC2873017},
	file = {PubMed Central Full Text PDF:C\:\\Users\\Minerva\\Zotero\\storage\\M54BMX8B\\Teuten et al. - 2009 - Transport and release of chemicals from plastics t.pdf:application/pdf},
