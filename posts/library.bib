@article{ahmed2019,
  title = {Long-{{Term Temperature Stress}} in the {{Coral Model Aiptasia Supports}} the "{{Anna Karenina Principle}}" for {{Bacterial Microbiomes}}},
  author = {Ahmed, Hanin Ibrahim and Herrera, Marcela and Liew, Yi Jin and Aranda, Manuel},
  date = {2019-05-08},
  journaltitle = {FRONTIERS IN MICROBIOLOGY},
  shortjournal = {Front. Microbiol.},
  volume = {10},
  pages = {975},
  publisher = {{Frontiers Media Sa}},
  location = {{Lausanne}},
  issn = {1664-302X},
  doi = {10.3389/fmicb.2019.00975},
  url = {https://www.webofscience.com/wos/woscc/summary/0210f60c-3f07-4991-acc0-50539d323514-b685c646/relevance/1},
  urldate = {2023-11-21},
  abstract = {The understanding of host-microbial partnerships has become a hot topic during the last decade as it has been shown that associated microbiota play critical roles in the host physiological functions and susceptibility to diseases. Moreover, the microbiome may contribute to host resilience to environmental stressors. The sea anemone Aiptasia is a good laboratory model system to study corals and their microbial symbiosis. In this regard, studying its bacterial microbiota provides a better understanding of cnidarian metaorganisms as a whole. Here, we Investigated the bacterial communities of different Aiptasia host-symbiont combinations under long-term heat stress In laboratory conditions. Following a 16S rRNA gene sequencing approach we were able to detect significant differences in the bacterial composition and structure of Aiptasia reared at different temperatures. A higher number of taxa (i.e., species richness), and consequently increased alpha-diversity and beta-dispersion, were observed in the microbiomes of heat-stressed individuals across all host strains and experimental batches. Our findings are In line with the recently proposed Anna Karenina principle (AKP) for animal microbiomes, which states that dysbiotic or stressed organisms have a more variable and unstable microbiome than healthy ones. Microbial Interactions affect the fitness and survival of their hosts, thus exploring the AKP effect on animal microbiomes is important to understand host resilience. Our data contributes to the current knowledge of the Aiptasia holobiont and to the growing field of study of host-associated microbiomes.},
  langid = {english},
  pagetotal = {11},
  keywords = {16S rRNA gene,aiptasia,beta-diversity,CLIMATE-CHANGE,COMMUNITIES,DISEASE,dispersion,DIVERSITY,HEALTH,microbiome,PH,REEF-BUILDING CORAL,RESILIENCE,SURFACE MUCUS,TAXONOMY,temperature},
  annotation = {Web of Science ID: WOS:000467308700002},
  file = {C:\Users\ssdon\Zotero\storage\ST2DF84U\Ahmed et al. - 2019 - Long-Term Temperature Stress in the Coral Model Ai.pdf}
}

