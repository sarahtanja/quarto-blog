	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1461-0248.2001.00203.x},
	keywords = {coral, coral reefs, global warming, Bleaching, Japan, Okinawa},
	file = {Full Text PDF:C\:\\Users\\Minerva\\Zotero\\storage\\CNGPMF2P\\Loya et al. - 2001 - Coral bleaching the winners and the losers.pdf:application/pdf;Snapshot:C\:\\Users\\Minerva\\Zotero\\storage\\5JN99MYZ\\j.1461-0248.2001.00203.html:text/html},
