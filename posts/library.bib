  file = {C\:\\Users\\ssdon\\Zotero\\storage\\YGR297YT\\Romera-Castillo et al. - 2018 - Dissolved organic carbon leaching from plastics st.pdf;C\:\\Users\\ssdon\\Zotero\\storage\\LYDQY8N2\\s41467-018-03798-5.html}
}

@article{rosado2019,
