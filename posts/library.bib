	keywords = {one-health},
	file = {Full Text PDF:C\:\\Users\\Minerva\\Zotero\\storage\\M6J2Z9YN\\Morrison et al. - 2022 - A growing crisis for One Health Impacts of plasti.pdf:application/pdf},
}

