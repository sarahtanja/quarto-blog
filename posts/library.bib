	number = {24},
	journaltitle = {Environ. Sci. Technol.},
	author = {Renaguli, Aikebaier and Fernando, Sujan and Holsen, Thomas M. and Hopke, Philip K. and Adams, Douglas H. and Balazs, George H. and Jones, T. Todd and Work, Thierry M. and Lynch, Jennifer M. and Crimmins, Bernard S.},
