@article{lesserPhylogeneticSignatureLight2019,
  title = {Phylogenetic Signature of Light and Thermal Stress for the Endosymbiotic Dinoflagellates of Corals ({{Family Symbiodiniaceae}})},
  author = {Lesser, Michael P.},
  date = {2019},
  journaltitle = {Limnology and Oceanography},
  volume = {64},
  number = {5},
