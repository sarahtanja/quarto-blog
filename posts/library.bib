@article{avesFirstEvidenceMicroplastics2022,
  title = {First Evidence of Microplastics in {{Antarctic}} Snow},
  author = {Aves, Alex R. and Revell, Laura E. and Gaw, Sally and Ruffell, Helena and Schuddeboom, Alex and Wotherspoon, Ngaire E. and LaRue, Michelle and McDonald, Adrian J.},
  date = {2022-06-07},
