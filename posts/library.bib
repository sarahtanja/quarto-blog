@article{wuEffectMicroplasticsExposure2019,
  title = {Effect of Microplastics Exposure on the Photosynthesis System of Freshwater Algae},
  author = {Wu, Yanmei and Guo, Peiyong and Zhang, Xiaoyan and Zhang, Yuxuan and Xie, Shuting and Deng, Jun},
  date = {2019-07-15},
  journaltitle = {Journal of Hazardous Materials},
