  author = {Baumgarten, Sebastian and Simakov, Oleg and Esherick, Lisl Y. and Liew, Yi Jin and Lehnert, Erik M. and Michell, Craig T. and Li, Yong and Hambleton, Elizabeth A. and Guse, Annika and Oates, Matt E. and Gough, Julian and Weis, Virginia M. and Aranda, Manuel and Pringle, John R. and Voolstra, Christian R.},
  date = {2015-09-22},
  journaltitle = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF AMERICA},
  shortjournal = {Proc. Natl. Acad. Sci. U. S. A.},
  volume = {112},
  number = {38},
  pages = {11893--11898},
  publisher = {{Natl Acad Sciences}},
  location = {{Washington}},
  issn = {0027-8424},
  doi = {10.1073/pnas.1513318112},
  url = {https://www.webofscience.com/wos/woscc/summary/0210f60c-3f07-4991-acc0-50539d323514-b685c646/relevance/1},
  urldate = {2023-11-21},
  abstract = {The most diverse marine ecosystems, coral reefs, depend upon a functional symbiosis between a cnidarian animal host (the coral) and intracellular photosynthetic dinoflagellate algae. The molecular and cellular mechanisms underlying this endosymbiosis are not well understood, in part because of the difficulties of experimental work with corals. The small sea anemone Aiptasia provides a tractable laboratory model for investigating these mechanisms. Here we report on the assembly and analysis of the Aiptasia genome, which will provide a foundation for future studies and has revealed several features that may be key to understanding the evolution and function of the endosymbiosis. These features include genomic rearrangements and taxonomically restricted genes that may be functionally related to the symbiosis, aspects of host dependence on alga-derived nutrients, a novel and expanded cnidarian-specific family of putative pattern-recognition receptors that might be involved in the animal-algal interactions, and extensive lineage-specific horizontal gene transfer. Extensive integration of genes of prokaryotic origin, including genes for antimicrobial peptides, presumably reflects an intimate association of the animal-algal pair also with its prokaryotic microbiome.},
  langid = {english},
  pagetotal = {6},
  keywords = {aiptasia,BASAL METAZOAN,CELL BIOLOGY,coral reefs,CULTURED ZOOXANTHELLAE,dinoflagellate,endosymbiosis,horizontal gene transfer,HORIZONTAL GENE-TRANSFER,INNATE IMMUNITY,MARINE-INVERTEBRATES,NEMATOSTELLA-VECTENSIS,pattern-recognition receptors,REPERTOIRE,SCLERACTINIAN CORAL,SYSTEM},
  annotation = {Web of Science ID: WOS:000361525100054},
  file = {C:\Users\ssdon\Zotero\storage\QQS9EXYH\Baumgarten et al. - 2015 - The genome of Aiptasia, a sea anemone model.pdf}
}

@incollection{bealeMetabolomicsReviewMultiOmicsBased2016,
  title = {Beyond {{Metabolomics}}: {{A Review}} of {{Multi-Omics-Based Approaches}}},
  shorttitle = {Beyond {{Metabolomics}}},
  booktitle = {Microbial {{Metabolomics}}: {{Applications}} in {{Clinical}}, {{Environmental}}, and {{Industrial Microbiology}}},
  author = {Beale, David J. and Karpe, Avinash V. and Ahmed, Warish},
  editor = {Beale, David J. and Kouremenos, Konstantinos A. and Palombo, Enzo A.},
