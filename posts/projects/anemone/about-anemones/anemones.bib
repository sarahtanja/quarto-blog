@article{wang2021,
  title = {The {{Active Phytohormone}} in {{Microalgae}}: {{The Characteristics}}, {{Efficient Detection}}, and {{Their Adversity Resistance Applications}}},
  shorttitle = {The {{Active Phytohormone}} in {{Microalgae}}},
  author = {Wang, Chun and Qi, Mei and Guo, Jiameng and Zhou, Chengxu and Yan, Xiaojun and Ruan, Roger and Cheng, Pengfei},
  date = {2021-12-22},
  journaltitle = {Molecules},
