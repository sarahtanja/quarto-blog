  langid = {english},
  keywords = {climate change,ecocline,flooding,growth rate,heterotrophy,temperature},
  file = {C:\Users\ssdon\Zotero\storage\2I2SXL92\Prelle and Karsten - 2022 - Photosynthesis, Respiration, and Growth of Five Be.pdf}
