}

@article{keeling2010,
  title = {The Endosymbiotic Origin, Diversification and Fate of Plastids},
  author = {Keeling, Patrick J.},
  date = {2010-03-12},
  journaltitle = {Philosophical Transactions of the Royal Society B: Biological Sciences},
  shortjournal = {Phil. Trans. R. Soc. B},
  volume = {365},
