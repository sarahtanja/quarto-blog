@article{barnes1992,
  title = {A Reappraisal of the Use of {{DMSO}} for the Extraction and Determination of Chlorophylls {\emph{a}} and {\emph{b}} in Lichens and Higher Plants},
  author = {Barnes, J. D. and Balaguer, L. and Manrique, E. and Elvira, S. and Davison, A. W.},
  date = {1992-04-01},
  journaltitle = {Environmental and Experimental Botany},
  shortjournal = {Environmental and Experimental Botany},
  volume = {32},
