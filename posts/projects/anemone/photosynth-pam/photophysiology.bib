  author = {Reich, Hannah G. and Tu, Wan-Chen and Rodriguez, Irene B. and Chou, Yalan and Keister, Elise F. and Kemp, Dustin W. and LaJeunesse, Todd C. and Ho, Tung-Yuan},
  date = {2021},
  journaltitle = {Journal of Phycology},
  volume = {57},
  number = {1},
