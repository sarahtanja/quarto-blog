@article{padilla-gamino2011,
  title = {Formation and Structural Organization of the Egg–Sperm Bundle of the Scleractinian Coral {{Montipora}} Capitata},
  author = {Padilla-Gamiño, J. L. and Weatherby, T. M. and Waller, R. G. and Gates, R. D.},
