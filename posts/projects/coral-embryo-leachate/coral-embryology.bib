@article{zhang2021,
  title = {Hazards of Phthalates ({{PAEs}}) Exposure: {{A}} Review of Aquatic Animal Toxicology Studies},
  shorttitle = {Hazards of Phthalates ({{PAEs}}) Exposure},
  author = {Zhang, Ying and Jiao, Yaqi and Li, Zixu and Tao, Yue and Yang, Yang},
  date = {2021-06-01},
  journaltitle = {Science of The Total Environment},
  shortjournal = {Science of The Total Environment},
  volume = {771},
