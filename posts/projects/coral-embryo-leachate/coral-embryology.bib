@article{stephens2022,
  title = {High-Quality Genome Assemblies from Key {{Hawaiian}} Coral Species},
  author = {Stephens, Timothy G and Lee, JunMo and Jeong, YuJin and Yoon, Hwan Su and Putnam, Hollie M and Majerová, Eva and Bhattacharya, Debashish},
  date = {2022-01-01},
  journaltitle = {GigaScience},
  shortjournal = {GigaScience},
