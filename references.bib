@ARTICLE{Gunaalan2020-sf,
  title        = {The hidden threat of plastic leachates: A critical review on
                  their impacts on aquatic organisms},
  shorttitle   = {The hidden threat of plastic leachates},
  author       = {Gunaalan, Kuddithamby and Fabbri, Elena and Capolupo, Marco},
