  month    =  feb,
  year     =  2020,
  language = "en"
}

@ARTICLE{Capolupo2020-qe,
  title    = "Chemical composition and ecotoxicity of plastic and car tire
              rubber leachates to aquatic organisms",
  author   = "Capolupo, Marco and Sørensen, Lisbet and Jayasena, Kongalage Don
              Ranil and Booth, Andy M and Fabbri, Elena",
  journal  = "Water Res.",
  volume   =  169,
  pages    =  115270,
  abstract = "Synthetic polymer-based materials are ubiquitous in aquatic
              environments, where weathering processes lead to their progressive
              fragmentation and the leaching of additive chemicals. The current
              study assessed the chemical content of freshwater and marine
              leachates produced from car tire rubber (CTR), polypropylene (PP),
              polyethylene terephthalate (PET), polystyrene (PS) and polyvinyl
