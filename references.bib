  language     = {en}
}

@ARTICLE{Mehdi2018-jg,
  title        = {Improving the coexistence of offshore wind farms and shipping:
                  an international comparison of navigational risk assessment
                  processes},
  shorttitle   = {Improving the coexistence of offshore wind farms and shipping},
  author       = {Mehdi, Raza Ali and Schröder-Hinrichs, Jens-Uwe and van
                  Overloop, Jeroen and Nilsson, Henrik and Pålsson, Jonas},
  journaltitle = {WMU journal of maritime affairs},
