}

@ARTICLE{Tait2003-ua,
  title    = "Is dimethylsulfoxide a reliable solvent for extracting chlorophyll
              under field conditions? Photosynth Res",
  author   = "Tait, Michelle and Hik, David",
  journal  = "Photosynth. Res.",
  volume   =  78,
  pages    = "87--91",
  abstract = "Dimethylsulfoxide (DMSO) appears to be a reliable solvent for
              extracting chlorophyll (Chl), however, modification of standard
              methods may be necessary for some species under field conditions.
              We found that Chl extraction of whole leaf tissue with DMSO
              incubated at between 25 and 40 degrees C was generally similar to
              the 80\% acetone method, except for one graminoid species that
              required maceration. There was little effect of incubation
