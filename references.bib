@ARTICLE{Dimond2017-mn,
  title        = {Photophysiology and hydrogen peroxide generation of the
                  dinoflagellate and chlorophyte symbionts of the sea anemone
                  Anthopleura elegantissima},
  author       = {Dimond, James L and Orechovesky, Shad and Oppenheimer, Jonas
