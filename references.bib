@ARTICLE{Lin2021-ds,
  title        = {Decline in plankton diversity and carbon flux with reduced sea
                  ice extent along the Western Antarctic Peninsula},
  author       = {Lin, Yajuan and Moreno, Carly and Marchetti, Adrian and
                  Ducklow, Hugh and Schofield, Oscar and Delage, Erwan and
