@ARTICLE{Bergschneider2008-dy,
  title    = "Nutritional Role of Two Algal Symbionts in the Temperate Sea
              Anemone Anthopleura elegantissima Brandt",
  author   = "Bergschneider, Heather and Muller-Parker, Gisèle",
  journal  = "Biol. Bull.",
  volume   =  215,
  number   =  1,
  pages    = "73--88",
  abstract = "The intertidal sea anemone Anthopleura elegantissima in the
              Pacific Northwest may host a single type of algal symbiont or two
              different algal symbionts simultaneously: zooxanthellae
              (Symbiodinium muscatinei) and zoochlorellae (green algae;
              Trebouxiophyceae, Chlorophyta). A seasonal comparison of
