@ARTICLE{Van_Oppen2017-tp,
  title    = "Shifting paradigms in restoration of the world's coral reefs",
  author   = "van Oppen, Madeleine J H and Gates, Ruth D and Blackall, Linda L
              and Cantin, Neal and Chakravarti, Leela J and Chan, Wing Y and
              Cormick, Craig and Crean, Angela and Damjanovic, Katarina and
