@ARTICLE{Aves2022-fv,
  title    = "First evidence of microplastics in Antarctic snow",
  author   = "Aves, Alex R and Revell, Laura E and Gaw, Sally and Ruffell,
              Helena and Schuddeboom, Alex and Wotherspoon, Ngaire E and LaRue,
              Michelle and McDonald, Adrian J",
  journal  = "Cryosphere",
  volume   =  16,
  number   =  6,
  pages    = "2127--2145",
  abstract = "Abstract. In recent years, airborne microplastics have been
