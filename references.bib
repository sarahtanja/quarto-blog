@ARTICLE{Mincarelli2021-gh,
  title        = {Consequences of combined exposure to thermal stress and the
                  plasticiser {DEHP} in Mytilus spp. differ by sex},
  author       = {Mincarelli, Luana Fiorella and Rotchell, Jeanette M and
                  Chapman, Emma C and Turner, Alexander P and Wollenberg Valero,
                  Katharina C},
  journaltitle = {Marine pollution bulletin},
  volume       = {170},
  pages        = {112624},
  date         = {2021-09},
  doi          = {10.1016/j.marpolbul.2021.112624},
  abstract     = {Little is known about the combined effect of environmental
                  factors and contaminants on commercially important marine
                  species, and whether this effect differs by sex. In this
                  study, blue mussels were exposed for seven days to both single
                  and combined stressors (i.e., +3 °C elevated temperature and
                  two environmentally relevant concentrations of the plastic
                  softener DEHP, 0.5 and 50 μg/l) in a factorial design. Males
                  were observed to be more sensitive to high temperature,
                  demonstrated by the significant increase in out-of-season
                  spawning gonads and higher gene expression of the antioxidant
                  catalase and the estrogen receptor genes. On the other hand,
                  while the gametogenesis cycle in females was more resilient
                  than in males, DEHP exposure altered the estrogen-related
                  receptor gene expression. We show that the combined stressors
                  DEHP and increased temperature, in environmentally relevant
                  magnitudes, have different consequences in male and female
                  mussels, with the potential to impact the timing and breeding
                  season success in Mytilus spp.},
  url          = {http://dx.doi.org/10.1016/j.marpolbul.2021.112624},
  urldate      = {2022-03-16},
  keywords     = {DEHP; Estrogen receptor-like; Gametogenesis cycle; Global
                  warming; Multiple stressors; Stress
                  response;phthalates;introduction;Import Apr 1},
  language     = {en}
}

