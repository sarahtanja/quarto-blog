@ARTICLE{Saunders1997-ps,
  title    = "The effects of temperature and light on two algal populations in
              the temperate sea anemone Anthopleura elegantissima (Brandt, 1835)",
  author   = "Saunders, Brian K and Muller-Parker, Gisèle",
