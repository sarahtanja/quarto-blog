  year     =  2020
}

@ARTICLE{Hughes2018-ef,
  title    = "Impact of nitrogen availability upon the electron requirement for
              carbon fixation in Australian coastal phytoplankton communities",
  author   = "Hughes, David J and Varkey, Deepa and Doblin, Martina A and
