@ARTICLE{Dutton2009-zb,
  title        = {Biogeographic variation in Mytilus galloprovincialis heat
                  shock gene expression across the eastern Pacific range},
  author       = {Dutton, Jessica M and Hofmann, Gretchen E},
  journaltitle = {Journal of experimental marine biology and ecology},
  volume       = {376},
  issue        = {1},
