@ARTICLE{McCarty2023-mq,
  title    = "Ecological risk assessment of tire and road wear particles: A
              preliminary screening for freshwater sources in Canada",
  author   = "McCarty, Kevin and Mian, Haroon R and Chhipi-Shrestha, Gyan and
