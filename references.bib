@ARTICLE{Perez-Rosales2021-lh,
  title        = {Mesophotic coral communities escape thermal coral bleaching in
                  French Polynesia},
  author       = {Pérez-Rosales, Gonzalo and Rouzé, Héloïse and Torda, Gergely
                  and Bongaerts, Pim and Pichon, Michel and {Under The Pole
