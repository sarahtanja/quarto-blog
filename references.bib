@ARTICLE{Cooke2010-kj,
  title        = {Making conservation physiology relevant to policy makers and
                  conservation practitioners: Making conservation physiology
                  relevant},
  shorttitle   = {Making conservation physiology relevant to policy makers and
                  conservation practitioners},
  author       = {Cooke, Steven J and O'Connor, Constance M},
