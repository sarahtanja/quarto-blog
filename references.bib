@ARTICLE{UnknownUnknown-hl,
  title    = "Interactive effects of elevated temperature and {pCO}(2) on
              early-life-history stages of the giant kelp Macrocystis pyrifera",
  author   = "Diego Gaitan-Espitia, Juan and Hancock, Joshua R and
