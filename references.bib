@ARTICLE{Padilla-Gamino2016-rk,
  title        = {Physiological plasticity and local adaptation to elevated
                  {pCO}(2) in calcareous algae: an ontogenetic and geographic
                  approach},
  shorttitle   = {Physiological plasticity and local adaptation to elevated
                  pCO(2) in calcareous algae},
  author       = {Padilla-Gamino, Jacqueline L and Diego Gaitan-Espitia, Juan
