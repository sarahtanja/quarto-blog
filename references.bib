@ARTICLE{Cato2019-mi,
  title    = "Genome-Wide Analysis of Cell Cycle-Regulating Genes in the
              Symbiotic Dinoflagellate Breviolum minutum",
  author   = "Cato, Michael L and Jester, Hallie D and Lavertu, Adam and Lyman,
