@ARTICLE{Takahashi2009-yf,
  title    = "How Does Cyclic Electron Flow Alleviate Photoinhibition in
              Arabidopsis?",
  author   = "Takahashi, Shunichi and Milward, Sara E and Fan, Da-Yong and Chow,
              Wah Soon and Badger, Murray R",
  journal  = "Plant Physiol.",
  volume   =  149,
  number   =  3,
  pages    = "1560--1567",
  abstract = "Cyclic electron flow (CEF) around photosystem I has a role in
              avoiding photoinhibition of photosystem II (PSII), which occurs
              under conditions in which the rate of photodamage to PSII exceeds
              the rate of its repair. However, the molecular mechanism
              underlying how CEF contributes to photoprotection is not yet well
