@ARTICLE{Rich2010-kl,
  title        = {A practical guide to understanding Kaplan-Meier curves},
  author       = {Rich, Jason T and Neely, J Gail and Paniello, Randal C and
                  Voelker, Courtney C J and Nussenbaum, Brian and Wang, Eric W},
  journaltitle = {Otolaryngology--head and neck surgery: official journal of
