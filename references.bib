@ARTICLE{Ouedraogo2023-pv,
  title    = "What are the toxicity thresholds of chemical pollutants for
              tropical reef-building corals? A systematic review",
  author   = "Ouédraogo, Dakis-Yaoba and Mell, Hugo and Perceval, Olivier and
              Burga, Karen and Domart-Coulon, Isabelle and Hédouin, Laetitia and
              Delaunay, Mathilde and Guillaume, Mireille M M and Castelin,
