}

@ARTICLE{Alcantar2024-pg,
  title        = {Examining the impacts of elevated, variable {pCO2} on larval
                  Pacific razor clams (Siliqua patula) in Alaska},
  author       = {Alcantar, Marina and Hetrick, Jeff and Ramsay, Jacqueline and
                  Kelley, Amanda},
  journaltitle = {Frontiers in marine science},
