  url          = {https://www.nature.com/articles/s42003-021-01961-1},
  urldate      = {2023-12-08},
  keywords     = {Import Apr 1},
  language     = {en}
}

@ARTICLE{Poopal2017-gb,
  title        = {Potential effects of low molecular weight phthalate esters
